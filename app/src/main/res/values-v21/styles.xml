<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ToolBarStyle" parent="Theme.AppCompat.NoActionBar">
        <item name="colorPrimary">#00044C</item>
        <item name="android:textColor">@color/black</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="colorButtonNormal">@color/colorPrimary</item>
        <item name="alertDialogTheme">@style/AlertDialogStyle_Default</item>
        <item name="android:textColorPrimary">@android:color/black</item>
        <item name="android:textColorSecondary">@android:color/black</item>
        <item name="android:statusBarColor">#00044C</item>
        <item name="actionMenuTextColor">@android:color/white</item>
        <item name="drawerArrowStyle">@style/DrawerArrowStyle</item>
        <item name="android:textColorHint">#d3d3d3</item>
